{
    "version": 2,
    "builds": [
      { "src": "netflix_site/wsgi.py", "use": "@vercel/python" },
      { "src": "static/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "netflix_site/wsgi.py" }
    ]
  }
  